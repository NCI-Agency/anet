{
  "extends": [
    "config:recommended",
    "group:test",
    "group:linters"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "labels": [
    "dependencies"
  ],
  "schedule": [
    "every weekday"
  ],
  "packageRules": [
    {
      "labels": [
        "devDependencies"
      ],
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "most non-major devDependencies",
      "groupSlug": "most-minor-dev-patch"
    },
    {
      "matchDepTypes": [
        "dependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "most non-major dependencies",
      "groupSlug": "most-minor-patch"
    },
    {
      "matchManagers": [
        "gradle"
      ],
      "matchDepTypes": [
        "dependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "most non-major Gradle dependencies",
      "groupSlug": "most-minor-gradle-patch"
    },
    {
      "labels": [
        "devDependencies"
      ],
      "matchManagers": [
        "gradle"
      ],
      "matchDepTypes": [
        "devDependencies",
        "plugin"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "most non-major Gradle devDependencies",
      "groupSlug": "most-minor-gradle-dev-patch"
    },
    {
      "matchPackageNames": [
        "chai",
        "react-router-dom",
        "com.graphql-java:graphql-java",
        "com.graphql-java:graphql-java-extended-scalars",
        "io.leangen.graphql:graphql-spqr-spring-boot-starter"
      ],
      "enabled": false
    },
    {
      "labels": [
        "devDependencies"
      ],
      "groupName": "NPM, dev-dependencies, Babel",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchPackageNames": [
        "@babel/{/,}**"
      ]
    },
    {
      "labels": [
        "devDependencies"
      ],
      "groupName": "NPM, dev-dependencies, GraphQL Code Generator",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchPackageNames": [
        "@graphql-codegen/{/,}**"
      ]
    },
    {
      "groupName": "NPM, dev-dependencies, WebdriverIO",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchPackageNames": [
        "@wdio/{/,}**",
        "webdriverio{/,}**"
      ]
    },
    {
      "groupName": "NPM, dependencies, Blueprint",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "dependencies"
      ],
      "matchPackageNames": [
        "@blueprintjs/{/,}**"
      ]
    },
    {
      "groupName": "NPM, dependencies, Emotion",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "dependencies"
      ],
      "matchPackageNames": [
        "@emotion/{/,}**"
      ]
    },
    {
      "groupName": "NPM, dependencies, Fullcalendar",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "dependencies"
      ],
      "matchPackageNames": [
        "@fullcalendar/{/,}**"
      ]
    },
    {
      "groupName": "NPM, dependencies, Slate",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "dependencies"
      ],
      "matchPackageNames": [
        "slate{/,}**"
      ]
    },
    {
      "groupName": "NPM, dependencies, Bootstrap",
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "dependencies"
      ],
      "matchPackageNames": [
        "bootstrap",
        "react-bootstrap"
      ]
    }
  ],
  "postUpdateOptions": [
    "yarnDedupeHighest"
  ],
  "vulnerabilityAlerts": {
    "labels": [
      "security"
    ]
  }
}
