  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.client.prod.js",
    "start": "export SERVER_URL=${SERVER_URL:-'http://localhost:8080'} DEV_PORT=${DEV_PORT:-3000} ; NODE_ENV=development webpack serve --config config/webpack.client.dev.js",
    "lint": "NODE_ENV=development eslint '.??*.js' '.storybook/*.js' '**/*.js'",
    "lint:fix": "yarn run lint --fix",
    "prettier:check": "NODE_ENV=development prettier-eslint --list-different '.??*.js' '.storybook/*.js' '**/*.js'",
    "prettier:format": "NODE_ENV=development prettier-eslint --write '.??*.js' '.storybook/*.js' '**/*.js'",
    "test": "echo 'Run \"test-all\" to execute all client-side tests, or pick a specific test target: \"test-jest\", \"test-wdio\", \"test-e2e\".' ; exit 1",
    "test-all": "yarn run test-jest && yarn run test-wdio && yarn run test-e2e",
    "test-e2e": "export SERVER_URL=${SERVER_URL:-'http://localhost:8180'} ; NODE_ENV=test ava --config ava.config.js",
    "test-wdio": "export SERVER_URL=${SERVER_URL:-'http://localhost:8180'} SUITE=${TEST_WDIO_SUITE:-withCustomFields} ; NODE_ENV=test wdio run ./config/wdio.config.js --suite base --suite ${SUITE}",
    "test-jest": "export ANET_CONFIG=../anet.yml ANET_DICTIONARY=../${ANET_DICTIONARY_NAME:-anet-dictionary.yml} ; NODE_ENV=test jest --testPathPattern='tests/(actions|reducers|models|utils)'",
    "test-all-noCF": "yarn run test-wdio-noCF",
    "test-wdio-noCF": "env TEST_WDIO_SUITE=noCustomFields yarn run test-wdio",
    "test-junit-report": "yarn run test-jest && yarn run test-wdio-junit && yarn run test-e2e-junit",
    "test-e2e-junit": "yarn run test-e2e -t | tap-xunit > ./build/test-results/e2e-junit-test-results.xml",
    "test-wdio-junit": "yarn run test-wdio ; yarn run wdio-junit-merge",
    "wdio-junit-merge": "./combine-test-results.sh ./build/test-results/wdio/*.xml > ./build/test-results/wdio-junit-test-results.xml",
    "test-wdio-noCF-junit": "yarn run test-wdio-noCF ; yarn run wdio-noCF-junit-merge",
    "wdio-noCF-junit-merge": "./combine-test-results.sh ./build/test-results/wdio/*.xml > ./build/test-results/wdio-noCF-junit-test-results.xml",
    "server-junit-merge": "./combine-test-results.sh ../build/test-results/test/*.xml > ./build/test-results/server-test-results.xml",
    "generate-graphql-schema": "graphql-codegen -p anet",
    "storybook": "export STORYBOOK_ANET_DICTIONARY=/${ANET_DICTIONARY_NAME:-anet-dictionary-storybook.yml} ; storybook dev -p 6006",
    "build-storybook": "export STORYBOOK_ANET_DICTIONARY=/${ANET_DICTIONARY_NAME:-anet-dictionary-storybook.yml} ; storybook build",
    "build-storybook-docs": "export STORYBOOK_ANET_DICTIONARY=/${ANET_DICTIONARY_NAME:-anet-dictionary-storybook.yml} ; storybook build --docs",
    "chromatic": "chromatic --project-token ${CHROMATIC_PROJECT_TOKEN}",
    "sim": "export ANET_CONFIG=../anet.yml ANET_DICTIONARY=../${ANET_DICTIONARY_NAME:-anet-dictionary.yml} SERVER_URL=${SERVER_URL:-'http://localhost:8080'} ; NODE_ENV=development webpack --config config/webpack.sim.dev.js && node build/anet.sim.dev.js"
  },
  "browserslist": [
    "> 1%",
